{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sourcemap": "ng build --prod --named-chunks --common-chunk --source-map --output-hashing=none",
    "prebabel:stats": "npm install angular-froala-wysiwyg --save && node node_modules/babel-cli/bin/babel.js node_modules/angular-froala-wysiwyg --out-dir node_modules/angular-froala-wysiwyg --presets es2015",
    "preclean:stats": "npm uninstall angular-froala-wysiwyg --save && npm install angular-froala-wysiwyg --save",
    "prelib:stats": "npm uninstall angular-froala-wysiwyg --save",
    "babel:stats": "npm run sourcemap && source-map-explorer --html dist/my-app/main.js > sourcemap/babel.html",
    "clean:stats": "npm run sourcemap && source-map-explorer --html dist/my-app/main.js > sourcemap/clean.html",
    "lib:stats": "npm run sourcemap && source-map-explorer --html dist/my-app/main.js > sourcemap/lib.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^2.0.51",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "froala-editor": "2.8.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.3.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "protractor": "~5.4.0",
    "source-map-explorer": "^1.5.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
